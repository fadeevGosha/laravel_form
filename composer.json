{
    "name": "symphonygf/laravel-form",
    "description": "This packages integrates Symfony Form Component in Laravel.",
    "keywords": ["laravel-form", "symphonygf-laravel", "symphonygf", "symphonygf-forms"],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8",
        "illuminate/support": "^6|^7|^8|^9",
        "symfony/form": "^4|^5|^6",
        "symfony/validator": "^4|^5|^6",
        "symfony/twig-bridge": "^4|^5|^6",
        "twig/twig": "^2.13|^3.3.8"
    },
    "autoload": {
        "psr-4": {
            "Yobidoyobi\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yobidoyobi\\Form\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yobidoyobi\\Form\\SymfonyFormProvider"
            ]
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.3",
        "orchestra/testbench": "^3|^4|^5|^6|^7"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "authors": [
        {
            "name": "Георгий",
            "email": "georgii.fadeev@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
